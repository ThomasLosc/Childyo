<div class="container-register m-auto mt-5 mb-5">
	<h1 class="text-center text-blue fw-bold">Modifier mes informations</h1>
</div>
{{ form_start(form) }}
<div class="container-fluid w-50 m-auto border rounded-3 p-3 mb-3">
	<div class="row mt-3">
		<div class="col-md-6 col-12 form-group-custom">
			{{ form_row(form.nom) }}
		</div>
		<div class="col-md-6 col-12 form-group-custom">
			{{ form_row(form.prenom) }}
		</div>
	</div>
	<div class="text-center mt-4">
		<button type="submit" class="primary-btn fw-bold">Modifier les informations</button>
	</div>
</div>
{{ form_end(form) }}
